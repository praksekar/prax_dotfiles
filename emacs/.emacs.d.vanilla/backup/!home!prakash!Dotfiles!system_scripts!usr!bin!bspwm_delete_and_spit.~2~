#!/bin/bash

spit_and_delete_entry() {
    child_node="$(bspc query -N -n)"
    if [ ! -z $(grep $child_node /tmp/swallow_nodes) ] 
    then
	parent_node=$(grep $child_node /tmp/swallow_nodes | head -n1 | awk '{print $1;}')
	bspc node $parent_node -s $child_node
	bspc node $child_node -c 
	sed -i '/$child_node/d' /tmp/swallow_nodes
    else
	bspc node -c
    fi
	
}

spit_and