#!/bin/bash

notify-send "wm_init executed"
source ~/.zshenv
sxhkd &
setxkbmap -option caps:swapescape
xcompmgr &
feh --bg-fill ~/Pictures/wallpapers/canyon.jpg #--randomize 
#hsetroot -fill "#000000"
bspwm_lemonbar &
#emacs --daemon

#notify-send "$TOUCHPAD"
xinput set-prop "$TOUCHPAD" 'libinput Tapping Enabled' 1
xinput set-prop "$TOUCHPAD" 'libinput Accel Speed' +0.5
