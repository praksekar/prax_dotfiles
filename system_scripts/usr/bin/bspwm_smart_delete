#!/bin/bash

child_window="$(bspc query -N -n focused)" 
terminal_window="$(bspc node -f @brother)"
if [ ! -z "$(grep $terminal_window /tmp/swallowed_nodes)" ]
then
    bspc node focused $terminal_window --swap $child_window
fi
    
bspc node $child_window -c
sed -i -e 's/$terminal_window//g' /tmp/swallowed_nodes

     
